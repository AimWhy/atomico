{
  "name": "@atomico/core",
  "description": "Atomico is a library for the creation of interfaces only using functions.",
  "version": "1.1.0",
  "main": "dist/atomico.js",
  "module": "dist/atomico.mjs",
  "source": "src/index.js",
  "author": "Matias Trujillo Olivares",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicojs/core.git"
  },
  "scripts": {
    "test": "jest",
    "build": "microbundle --name @atomico/core",
    "watch": "microbundle -w --name @atomico/core",
    "prepare": "npm run build && npm test",
    "upload": "npm publish --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "microbundle": "^0.9.0",
    "regenerator-runtime": "^0.13.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "pragma": "h",
          "throwIfNamespace": false
        }
      ]
    ]
  }
}
