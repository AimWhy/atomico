{
    "name": "atomico",
    "version": "0.23.3",
    "description": "Atomico is a small library for the creation of interfaces based on web-components, only using functions and hooks.",
    "main": "core.js",
    "module": "core.js",
    "source": "src/core/core.js",
    "directories": {
        "docs": "docs"
    },
    "scripts": {
        "test": "web-test-runner src/**/*.test.{js,jsx} ",
        "test:w": "web-test-runner src/**/*.test.{js,jsx}  --watch",
        "build:html": "rollup -c",
        "prepare": "npm run build:html && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/atomicojs/atomico.git"
    },
    "keywords": [
        "jsx",
        "web-components",
        "hooks",
        "custom-elements",
        "webcomponent",
        "react"
    ],
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/atomicojs/atomico/issues"
    },
    "homepage": "https://github.com/atomicojs/atomico#readme",
    "devDependencies": {
        "@bundled-es-modules/chai": "^4.2.2",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-sucrase": "^3.1.0",
        "@web/dev-server-esbuild": "^0.1.4",
        "@web/dev-server-rollup": "^0.1.5",
        "@web/test-runner": "^0.6.45",
        "htm": "^3.0.4",
        "rollup": "^2.18.0"
    },
    "files": [
        "/docs",
        "/cjs",
        "/src",
        "/core.js",
        "/core.js.map",
        "/html.js",
        "/html.js.map",
        "/index.d.ts"
    ],
    "typings": "index.d.ts",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "exports": {
        ".": "./core.js",
        "./html": "./html.js"
    }
}
