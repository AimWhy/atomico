{
    "name": "atomico",
    "version": "0.21.0",
    "description": "Atomico is a small library for the creation of interfaces based on web-components, only using functions and hooks.",
    "main": "core.js",
    "module": "core.js",
    "source": "src/core/core.js",
    "directories": {
        "docs": "docs"
    },
    "scripts": {
        "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "prepare": "npm run build && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/atomicojs/atomico.git"
    },
    "keywords": [
        "jsx",
        "web-components",
        "hooks"
    ],
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/atomicojs/atomico/issues"
    },
    "homepage": "https://github.com/atomicojs/atomico#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@rollup/plugin-sucrase": "^3.0.0",
        "htm": "^3.0.3",
        "jasmine-core": "^3.5.0",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^2.0.1",
        "karma-rollup-preprocessor": "^7.0.5",
        "karma-spec-reporter": "^0.0.32",
        "rollup": "^1.32.1"
    },
    "files": [
        "/docs",
        "/cjs",
        "/src",
        "/chunk",
        "/core.js",
        "/core.js.map",
        "/html.js",
        "/html.js.map",
        "/index.d.ts"
    ],
    "typings": "index.d.ts",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "exports": {
        "require": "./cjs/core.js",
        "default": "./core.js"
    }
}
