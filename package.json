{
	"name": "atomico",
	"version": "0.8.10",
	"description": "Atomico is a small library for the creation of interfaces based on web-components, only using functions and hooks.",
	"main": "index.js",
	"module": "index.js",
	"directories": {
		"docs": "docs"
	},
	"scripts": {
		"test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"prepare": "npm run build && npm run test"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/atomicojs/atomico.git"
	},
	"keywords": [
		"jsx",
		"web-components",
		"hooks"
	],
	"author": "UpperCod",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/atomicojs/atomico/issues"
	},
	"homepage": "https://github.com/atomicojs/atomico#readme",
	"devDependencies": {
		"@atomico/rollup-plugin-sizes": "^1.0.3",
		"htm": "^2.1.1",
		"jasmine-core": "^3.4.0",
		"karma": "^4.0.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-jasmine": "^2.0.1",
		"karma-rollup-preprocessor": "^7.0.0",
		"karma-spec-reporter": "^0.0.32",
		"rollup": "^1.15.3",
		"rollup-plugin-node-resolve": "^5.0.2",
		"rollup-plugin-sucrase": "^2.1.0",
		"rollup-plugin-terser": "^5.0.0"
	},
	"dependencies": {},
	"files": [
		"src",
		"core.js",
		"index.js",
		"router.js",
		"lazy.js",
		"html.js"
	]
}
