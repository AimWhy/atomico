{
  "include": [
      "src/**/*", 
      "types/**/*",
      "./html.js",
      "./jsx-runtime.js"
  ],
  "exclude": ["src/**/tests/","tests/"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "checkJs": true,
    "jsxImportSource": "../",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "",
    "lib": ["ESNext", "DOM","DOM.Iterable"],
    "paths":{
      "atomico":["./types/core.d.ts"],
    },
  }
}